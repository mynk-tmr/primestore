	"compilerOptions": {
		"composite": true,
		"declaration": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"emitDeclarationOnly": true,
		"types": ["node", "bun"],
		"jsx": "react-jsx",
